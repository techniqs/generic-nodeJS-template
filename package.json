{
  "name": "generic",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "build": "babel ./src -d dist",
    "clean": "rm -rf dist",
    "dev": "nodemon --exec babel-node --presets @babel/preset-env src/index.js",
    "start:prod": "npm run clean && npm run build && node dist/index.js"
  },
  "dependencies": {
    "cors": "2.8.5",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@babel/cli": "7.13.14",
    "@babel/core": "7.13.15",
    "@babel/node": "7.13.13",
    "@babel/preset-env": "7.13.15",
    "nodemon": "2.0.7"
  }
}
